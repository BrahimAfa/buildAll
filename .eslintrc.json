{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [ "airbnb-base"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": ["warn", 4],
		"no-console": "warn",
		"prefer-const": "warn",
		"object-curly-newline": "off"
    }
}

